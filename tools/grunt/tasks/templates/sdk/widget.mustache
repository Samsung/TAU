<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8"/>
	<meta http-equiv="X-UA-Compatible" content="IE=9" />
	<link rel="stylesheet" type="text/css" href="{{basedir}}../css/styles.css" />
	<link rel="stylesheet" type="text/css" href="{{basedir}}../css/snippet.css" />
	<script type="text/javascript" src="{{basedir}}../scripts/snippet.js"></script>
	<script type="text/javascript" src="{{basedir}}../scripts/jquery.util.js" charset="utf-8"></script>
	<script type="text/javascript" src="{{basedir}}../scripts/common.js" charset="utf-8"></script>
	<script type="text/javascript" src="{{basedir}}../scripts/core.js" charset="utf-8"></script>
	<script type="text/javascript" src="{{basedir}}../scripts/search.js" charset="utf-8"></script>

	<title>{{title}}</title>
</head>

<body onload="prettyPrint()" style="overflow: auto;">

<div id="navigation">
	<div id="profile">
		<p>{{#mobile}}<img alt="Mobile Web" src="{{basedir}}../images/mobile_s_w.png"/>{{/mobile}}
            {{#wearable}}<img alt="Wearable Web" src="{{basedir}}../images/wearable_s_w.png"/>{{/wearable}}</p>
	</div>

    <div id="toc_border"><div id="toc">
        <p class="toc-title">Related Info</p>
        <ul class="toc">
            <li><a href="{{basedir}}../../html/ui_fw_api/ui_fw_api_cover.htm">Tizen Advanced UI Framework (TAU) API</a></li>
        </ul>
    </div></div>
</div>


<div id="container"><div id="contents"><div class="content">
<h1>{{title}}</h1>

{{{brief}}}
{{#since}}
<table class="note">
	<tbody>
	<tr>
		<th class="note">Note</th>
	</tr>
	<tr>
		<td class="note">This component is supported since <strong>{{since}}</strong>.</td>
	</tr>
	</tbody>
</table>
{{/since}}

{{#extends}}
	<p>Widget extends: <a href="{{{extendsFile}}}">{{{extends}}}</a></p>
{{/extends}}

{{#customElementName}}
	<h3>Custom element</h3>
	<pre class="prettyprint">&lt;{{{customElementName}}} ...&gt; ... &lt;/{{{customElementName}}}&gt;</pre>
{{/customElementName}}

<h2>Table of Contents</h2>
<ol class="toc">
	{{#toc}}
		<li><a href="#{{href}}">{{name}}</a>
			{{#hasTOC}}
				<ol class="toc">
					{{#toc}}
						<li><a href="#{{href}}">{{name}}</a>
						</li>
					{{/toc}}
				</ol>
			{{/hasTOC}}
		</li>
	{{/toc}}
</ol>

{{{description}}}


{{#showOptions}}
	<h2><a id="options-list"></a>Options</h2>

	<table>
		<tbody>
		<tr>
			<th>Option</th>
			<th>Custom element attribute</th>
			<th>Type</th>
			<th>Default value</th>
			<th>Description</th>
		</tr>
		{{#options}}
			{{#name}}
			<tr>
				<td class="option"><span style="font-family: Courier New,Courier,monospace">data-{{name}}</span></td>
				<td class="option"><span style="font-family: Courier New,Courier,monospace">{{name}}</span></td>
				<td class="option">{{#types}}{{types}}{{/types}}</td>
				<td class="option">{{defaultValue}}</td>
				<td class="option">{{{description}}}</td>
			</tr>
			{{/name}}
		{{/options}}
		</tbody>
	</table>
{{/showOptions}}
{{{descriptions.option}}}
{{#showEvents}}
	<h2><a id="events-list"></a>Events</h2>

	<table>
		<tbody>
		<tr>
			<th>Name</th>
			<th>Description</th>
		</tr>
		{{#events}}
			<tr>
				<td class="option"><span style="font-family: Courier New,Courier,monospace">{{name}}</span></td>
				<td>{{{description}}}</td>
			</tr>
		{{/events}}
		</tbody>
	</table>

{{/showEvents}}
{{#showMethods}}
	<h2><a id="methods-list"></a>Methods</h2>
	<table class="informaltable">
		<thead>
		<tr>
			<th>Method</th>
			<th>Description</th>
		</tr>
		</thead>
		<tbody>
		{{#methods}}
			{{#isPublic}}
				<tr>
					<td>
						<pre class="intable prettyprint">{{#return.types}}{{return.types}} {{/return.types}}<a href="#method-{{name}}">{{name}}</a> ( {{#params}}{{types}}{{#isOptional}}?{{/isOptional}} {{name}}{{#defaultValue}} = {{defaultValue}}{{/defaultValue}}{{^isLast}}, {{/isLast}}{{/params}} ) </pre>
					</td>
					<td>{{{brief}}}</td>
				</tr>
			{{/isPublic}}
		{{/methods}}
		</tbody>
	</table>
	<dl>
{{#methods}}
			{{#isPublic}}
				<dt class="method">
					<code><b><span class="methodName" id="method-{{name}}">{{name}}</span></b></code>
				</dt>
				<dd>
					<div class="brief">
						{{{brief}}}
					</div>
					<div class="synopsis">
						<pre class="signature prettyprint">{{#return.types}}{{return.types}} {{/return.types}}{{name}} ( {{#params}}{{types}}{{#isOptional}}?{{/isOptional}} {{name}}{{#defaultValue}} = {{defaultValue}}{{/defaultValue}}{{^isLast}}, {{/isLast}}{{/params}}) </pre>
					</div>
					{{#since}}
						<p><span class="version">Since: </span>
							{{since}}
						</p>
					{{/since}}
					<div class="description">
						{{{description}}}
					</div>
					{{#hasParams}}
					<div class="parameters">
						<p><span class="param">Parameters:</span></p>
					<table>
					<tbody>
					<tr>
						<th>Parameter</th>
						<th>Type</th>
						<th>Required / optional</th>
						<th>Default value</th>
						<th>Description</th>
					</tr>
					{{/hasParams}}
					{{#params}}
					<tr>
						<td><span style="font-family: Courier New,Courier,monospace">{{name}}</span></td>
						<td>{{types}}</td>
						<td>{{#isOptional}}optional{{/isOptional}}{{^isOptional}}required{{/isOptional}}</td>
						<td>{{defaultValue}}</td>
						<td>{{{description}}}</td>
					</tr>
					{{/params}}
					{{#hasParams}}
					</tbody>
					</table>
					</div>
					{{/hasParams}}
					<div class="returntype">
						<p><span class="return">Return value:</span></p>
						{{#return}}
							<table>
								<tbody>
								<tr>
									<th>Type</th>
									<th>Description</th>
								</tr>
								<tr>
									<td>{{types}}</td>
									<td>{{{description}}}</td>
								</tr>
								</tbody>
							</table>
						{{/return}}
						{{^return}}
							No Return Value
						{{/return}}
					</div>
					{{#examples}}
						<div class="example">
							<span class="example"><p>Code
								example:</p><p>{{name}}</p></span>
							<pre name="code {{#styles}}{{.}}{{/styles}}" class="examplecode
							prettyprint">{{{code}}}</pre><span class="types">{{#type}}{{.}}{{/type}}</span>
						</div>
					{{/examples}}
				</dd>
			{{/isPublic}}
{{/methods}}
		</dl>
	{{/showMethods}}
<script type="text/javascript" src="{{basedir}}../scripts/jquery.zclip.min.js"></script>
<script type="text/javascript" src="{{basedir}}../scripts/showhide.js"></script>
</div></div></div>

