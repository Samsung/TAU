  -moz-mask-size: 100% 100%;
  -ms-mask-size: 100% 100%;
  -o-mask-size: 100% 100%;
  mask-size: 100% 100%;
  -webkit-mask-position: 0;
  -moz-mask-position: 0;
  -ms-mask-position: 0;
  -o-mask-position: 0;
  mask-position: 0;
  animation-fill-mode: both;
  background-color: W014L4;
  position: absolute;
  left: 0;
}
